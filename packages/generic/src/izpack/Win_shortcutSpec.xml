<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
<skipIfNotSupported/>
<programGroup defaultName="nanim" location="applications"/>
<shortcut name="nanimstudio"
        target="$JAVA_HOME\bin\javaw.exe"
	commandLine="-jar  &quot;$INSTALL_PATH\nanimstudio-${nanim_version}.jar &quot;"
        description="Launch nanimstudio"
        workingDirectory="$INSTALL_PATH"
        iconFile="$INSTALL_PATH\icon.ico"
        initialState="normal"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="no"
        startup="no">
	<createForPack name="Core"/>
</shortcut>

<shortcut
 		name="Uninstall nanim softwares"
		target="$JAVA_HOME\bin\javaw.exe"
		workingDirectory=""
		commandLine="-jar &quot;$INSTALL_PATH\Uninstaller\uninstaller.jar&quot;"
 		description="Run me with administrator privilege to uninstall nanim softwares"
 		initialState="normal"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no"
		startup="no" >
	<createForPack name="Core"/>
</shortcut>


</shortcuts>
